{
  "name": "NotesApp",
  "version": "0.0.1",
  "description": "Notes application",
  "scripts": {
    "start": "babel-node server/app.js",
    "server": "babel-node server/app.js",
    "build": "webpack",
    "devserver": "webpack-dev-server --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8090 --host 0.0.0.0"
  },
  "author": {
    "name": "Kateryna Porshnieva",
    "email": "k.porshnieva@gmail.com",
    "url": "https://github.com/krambertech"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "connect-multiparty": "^2.0.0",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "less": "^2.7.2",
    "materialize-css": "^0.99.0",
    "mongodb": "^2.2.30",
    "mongoose": "^4.11.3",
    "prop-types": "^15.5.10",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "react-dropzone": "^3.13.3",
    "react-flexbox-grid": "^1.1.3",
    "react-masonry-component": "^4.4.0",
    "react-materialize": "^1.0.4",
    "react-md": "^1.0.16",
    "react-router": "^3.0.0",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "superagent": "^3.5.2"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "html-webpack-plugin": "^2.29.0",
    "rimraf": "^2.6.1",
    "svg-inline-loader": "^0.8.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^5.8.38",
    "babel-loader": "^5.4.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less-loader": "^2.2.3",
    "react-hot-loader": "^1.3.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "allowImportExportEverywhere": true
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off",
      "import/prefer-default-export": "off",
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "arrow-parens": "off",
      "no-mixed-operators": "off",
      "import/imports-first": "off",
      "no-useless-escape": "off",
      "no-extra-boolean-cast": "off",
      "jsx-a11y/href-no-hash": "off",
      "react/no-string-refs": "off",
      "no-prototype-builtins": "off",
      "react/no-unused-prop-types": "off",
      "jsx-a11y/label-has-for": "off",
      "no-plusplus": "off",
      "no-lonely-if": "off",
      "react/self-closing-comp": "off",
      "object-shorthand": [
        "error",
        "always",
        {
          "avoidQuotes": false
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "import/extensions": [
        "off",
        "never"
      ]
    }
  }
}
